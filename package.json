{
  "name": "chakra-radio-group",
  "version": "1.1.1",
  "description": "Custom radio group component built with Chakra-UI",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "sudo-labs",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/chakra-radio-group.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudolabs-io/chakra-radio-group.git"
  },
  "keywords": [
    "react",
    "chakra",
    "chakra-radio",
    "chakra-radio-group",
    "radio-group"
  ],
  "homepage": "https://github.com/sudolabs-io/chakra-radio-group#readme",
  "bugs": {
    "url": "https://github.com/sudolabs-io/chakra-radio-group/issues"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "@chakra-ui/pro-theme": "0.0.57",
    "@chakra-ui/react": "2.3.6",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "7.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-render-prop-type": "^0.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@chakra-ui/pro-theme": "0.0.57",
    "@chakra-ui/react": "2.3.6",
    "@chakra-ui/storybook-addon": "4.0.12",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.6",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addons": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "babel-loader": "^8.2.5",
    "framer-motion": "7.5.3",
    "husky": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-render-prop-type": "^0.1.0",
    "semantic-release": "^19.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "4.6.4"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "chore",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "chore"
              },
              {
                "type": "docs"
              },
              {
                "type": "style",
                "hidden": true
              },
              {
                "type": "refactor"
              },
              {
                "type": "perf"
              },
              {
                "type": "test"
              }
            ]
          }
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version}"
        }
      ]
    ]
  }
}
